<!DOCTYPE html>
<html>
  <head>
    <link rel="shortcut icon" href="/assets/favicon.ico" />
    <%= display_meta_tags(default_meta_tags) %>
    <meta name="viewport" content="user-scalable=no, initial-scale=1.0, maximum-scale=1.0" />
    <%= stylesheet_link_tag    admin_namespace? ? "application_admin" : "application", :media => "all" %>
    <link type="text/css" rel="stylesheet" href="http://fast.fonts.net/cssapi/8cef5d75-4d5b-4e51-8f0e-49548f73883f.css"/>
    <%= javascript_include_tag admin_namespace? ? "application_admin" : "application" %>
    <%= csrf_meta_tags %>
    <script src="//load.sumome.com/" data-sumo-site-id="5c6831a22972a3351468dcbc4017a52cd97ffb9040338b3cc3c06984ae29d5af" async="async"></script>
  </head>
  <body id="<%= controller.controller_name + "_" + controller.action_name %>" class="<%= namespace %>">
  	<div class="container_12" id="wrapper">
         <!-- <div id="notifications">
            <p class="notice"><%= notice %></p>
            <p class="alert"><%= alert %></p>
         </div> -->
  		<%= render "home/header" %>

  		<div id="main">
        <% flash.each do |name, msg| -%>
          <%= content_tag :div, msg, class: name %>
        <% end -%>

  			<%= render "admin/shared/header" if admin_namespace? %>
  			<%= yield %>
  			<%= render "admin/shared/box_dashboard" if admin_namespace? %>
  		</div>

  	</div>

    <%= render "home/footer" %>
    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-69838097-1', 'auto');
      ga('send', 'pageview');
    </script>
  </body>
</html>
